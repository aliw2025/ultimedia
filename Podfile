target 'Ultimedia' do

  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Ultimedia
  pod "AMSMB2"
  pod "Gifu"
  pod "TVVLCKit", "~> 3.4.0"

end
